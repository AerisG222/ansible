ASPNETCORE_ENVIRONMENT={{ mawus_auth__aspnet_env }}
GOOGLE_APPLICATION_CREDENTIALS={{ mawus_auth__google_email_creds_file }}

MAW_AUTH_Kestrel__Certificates__Default__Path={{ mawus_auth__pfx_auth }}
MAW_AUTH_Kestrel__Certificates__Default__Password={{ mawus_auth__pfx_pwd_auth }}

MAW_AUTH_DataProtection__Path={% if mawus_auth__dataprotection_dir | length > 0 %}{{ mawus_auth__dataprotection_dir }}{% else %}/dataprotection{% endif %}

MAW_AUTH_Environment__DbConnectionString=Server=127.0.0.1;Port=5432;Database=maw_website;Max Auto Prepare=100;User Id={{ mawus_auth__sql_username }};Password={{ mawus_auth__sql_password }}
MAW_AUTH_Environment__IdsrvDbConnectionString=Server=127.0.0.1;Port=5432;Database=idsrv;Max Auto Prepare=100;User Id={{ mawus_auth__sql_username }};Password={{ mawus_auth__sql_password }}
MAW_AUTH_Environment__WwwClientSecret={{ mawus_auth__www_auth_client_secret }}

MAW_AUTH_GooglePlus__ClientId={{ mawus_auth__google_plus_client_id }}
MAW_AUTH_GooglePlus__ClientSecret={{ mawus_auth__google_plus_client_secret }}

MAW_AUTH_Twitter__ConsumerKey={{ mawus_auth__twitter_client_id }}
MAW_AUTH_Twitter__ConsumerSecret={{ mawus_auth__twitter_client_secret }}

MAW_AUTH_Microsoft__ApplicationId={{ mawus_auth__microsoft_client_id }}
MAW_AUTH_Microsoft__Secret={{ mawus_auth__microsoft_client_secret }}

MAW_AUTH_GitHub__ClientId={{ mawus_auth__github_client_id }}
MAW_AUTH_GitHub__ClientSecret={{ mawus_auth__github_client_secret }}

MAW_AUTH_SigningCertDir={{ mawus_auth__pfx_signing | dirname }}
